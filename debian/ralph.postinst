#!/bin/sh

set -e

case "$1" in
    configure)
    ;;

    triggered)
        /opt/ralph/ralph-core/bin/ralph migrate
        /opt/ralph/ralph-core/bin/ralph sitetree_resync_apps
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0
