[req]
default_bits = 2048
prompt = no
default_md = sha256
x509_extensions = v3_req
distinguished_name = wildcard_lewitowanie.com.pl
[ wildcard_lewitowanie.com.pl ]
C = US
ST = CA
L = San Francisco
O = My Company
CN = *.lewitowanie.com.pl
emailAddress = example@lewitowanie.com.pl
[ v3_req ]
keyUsage = keyEncipherment, dataEncipherment
extendedKeyUsage = serverAuth
subjectAltName = @alt_names
[alt_names]
DNS.1 = www.lewitowanie.com.pl
